{
  "name": "fairway/canto-saas-api",
  "type": "library",
  "description": "Client library for Canto SaaS APIs.",
  "homepage": "https://github.com/ecentral/fairway-canto-saas-api/",
  "license": "MIT",
  "keywords": [
    "Canto",
    "API"
  ],
  "authors": [
    {
      "name": "Tim Schreiner",
      "role": "Developer",
      "email": "tim.schreiner@km2.de"
    },
    {
      "name": "Christian Rodriguez Benthake",
      "role": "Developer",
      "email": "c.rodriguez.benthake@ecentral.de"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "psr/log": "^1.1 || ^3.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.2",
    "friendsofphp/php-cs-fixer": "^3.3",
    "phpro/grumphp": "^1.5",
    "guzzlehttp/guzzle": "^6.3 || ^7.3"
  },
  "autoload": {
    "psr-4": {
      "Fairway\\CantoSaasApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fairway\\CantoSaasApi\\Tests\\": "tests"
    }
  },
  "suggest": {
    "guzzlehttp/guzzle": "Recommended default implementation of the psr-18 http-client interface."
  }
}
